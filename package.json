{
  "name": "node-grpc-typescript",
  "version": "0.1.0",
  "description": "Node.js gRPC Structure for TypeScript Example",
  "main": "dist/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --ext .ts .",
    "build": "rimraf dist && tsc",
    "build:proto": "node bin/proto",
    "prestart": "npm run lint && npm run build",
    "start": "node dist/server",
    "client": "node dist/client",
    "health": "node dist/health",
    "migrate": "yarn knex migrate:latest --knexfile src/database/knexfile.js",
    "seed": "babel-node node_modules/.bin/knex seed:run --knexfile src/database/knexfile.js",
    "make:seeder": "yarn knex seed:make --knexfile src/database/knexfile.js -x ts",
    "make:migration": "yarn knex migrate:make --knexfile src/database/knexfile.js -x ts",
    "start:dev": "nodemon --watch 'src/**/*.ts' --exec 'npm run build && nodemon dist/server' --verbose -e ts"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "camelcase-keys": "^6.2.2",
    "dotenv": "^8.2.0",
    "google-protobuf": "^3.12.2",
    "googleapis": "^59.0.0",
    "grpc": "^1.24.3",
    "knex": "^0.21.2",
    "objection": "^2.2.2",
    "pg": "^8.3.0",
    "snakecase-keys": "^3.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.11.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.0.0",
    "@types/google-protobuf": "^3.7.2",
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "dtslint": "^3.6.12",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "grpc-tools": "^1.9.0",
    "grpc_tools_node_protoc_ts": "^4.1.0",
    "nodemon": "^1.18.6",
    "protobufjs": "^6.10.1",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CatsMiaow/node-grpc-typescript.git"
  },
  "keywords": [
    "Node.js",
    "gRPC",
    "TypeScript"
  ],
  "homepage": "https://github.com/CatsMiaow/node-grpc-typescript#readme",
  "author": "CatsMiaow",
  "license": "MIT"
}
